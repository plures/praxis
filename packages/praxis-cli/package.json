{
  "name": "@plures/praxis-cli",
  "version": "0.1.0",
  "description": "Command-line interface for the Praxis application framework - scaffolding, generators, and validation tools",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "praxis-cli": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "praxis",
    "cli",
    "generator",
    "scaffolding",
    "code-generation",
    "typescript"
  ],
  "author": "Plures",
  "license": "MIT",
  "dependencies": {
    "@plures/praxis-core": "workspace:*",
    "commander": "^14.0.2",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "publishConfig": {
    "access": "public"
  }
}
